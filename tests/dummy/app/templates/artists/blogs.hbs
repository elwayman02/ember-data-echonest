<table>
  <tr>
    <th>Blog Title</th>
    <th>Source URL</th>
    <th>Blog Content</th>
    <th>Date Found</th>
    <th>Date Posted</th>
  </tr>
  {{#each model as |blog|}}
    <tr>
      <td>{{blog.name}}</td>
      <td>{{blog.url}}</td>
      <td>{{blog.summary}}</td>
      <td>{{blog.dateFound}}</td>
      <td>{{blog.datePosted}}</td>
    </tr>
  {{/each}}
</table>
