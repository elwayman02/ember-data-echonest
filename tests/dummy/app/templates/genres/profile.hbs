<table>
    <tr>
        <th>Genre</th>
        <th>Urls</th>
        <th>Description</th>
    </tr>
    {{#each model as |genre|}}
        <tr>
            <td>{{genre.name}}</td>
            <td><ul>{{#each-in genre.urls as |type url|}}
                <li>{{url}}</li>
            {{/each-in}}</ul></td>
            <td>{{genre.description}}</td>
        </tr>
    {{/each}}
</table>